n, k = map(int, input().split())

data = [] #동전 입력
result = 0 # 최소 필요한 동전

#k 보다 작은 동전 분류
for i in range(n):
    coin = int(input())
    if coin <= k:
        data.append(coin)

for i in range(1, len(data)+1):
    coin = data[-i]
    if coin <= k:
        sum = k // coin
        result += sum
        k -= coin*sum

print(result)
